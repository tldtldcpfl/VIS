from argparse import ArgumentParser

from mmdet.core import coco_eval



def main ():
  parser = ArgumentParser(description = 'COCO Evaluation' )
  parser.add_argument('result', help='result file path' )
  parser.add_argument('--ann', help ='anntation file patch' )
  parser.add_argument(
      '---types',
      type=str
      nargs='+',
      choices=['proposal_fast','proposal', 'bbox', 'segm', 'keypoint'],
      default=['bbox'],
      help='result types')
  parser.add _ argument(
      '--max-dets', 
      type=int,
      nargs='+',
      default=[100,300,1000],
      help='proposal numbers, only used for recall evaluation' )
  arg = parser.parse_args()
  coco_eval(args.result, args.types, args.ann, args.mas_dets)


  if __name__ == '__main__':
    main()
